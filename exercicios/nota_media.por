programa {
  funcao inicio() {
        // programa de calcular média

    real n1, n2, n3, media 
    
    escreva("digite sua primeira nota: ")
    leia(n1)

    escreva("digite sua segunda nota: ")
    leia(n2)

    escreva("digite sua terceira nota: ")
    leia(n3)

    media = (n1 + n2 + n3) / 3

    escreva("sua média foi: ", media)
  }
}
