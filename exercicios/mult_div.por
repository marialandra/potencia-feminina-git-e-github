programa {
  funcao inicio() {
    real a, b, multiplica, divide

    escreva("por favor, escreva o valor de A: ")
    leia(a)

    escreva("por favor, leia o valor de B: ")
    leia(b)

    multiplica = a * b 
    divide = a / b

    escreva("na multiplicação, o resultado de A e B é: ",multiplica, "\n")
    escreva("na divisão, o resultado de A e B é: ", divide, "\n")

  }
}
