programa {
  funcao inicio() {
    real a, b, c, p, exp
    
    a = 2 + 3  //soma
    b = 6 - 2  //subtrai
    c = 5 * 5  //multiplica
    p = 2 ^ 10  //potencia
    exp = a + b + c + p  //expressão aritimética

    escreva("\nvalor de A: ",a)
    escreva("\nvalor de B: ",b)
    escreva("\nvalor de C: ",c)
    escreva("\nvalor de P: ",p)
    escreva("\nvalor de EXPRESSÃO: ",exp)

  }
}
